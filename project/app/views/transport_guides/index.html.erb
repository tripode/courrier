<h1>Listing transport_guides</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Foreign company</th>
    <th>Employee</th>
    <th>Service type</th>
    <th>Date</th>
    <th>State</th>
    <th>Payment method</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @transport_guides.each do |transport_guide| %>
  <tr>
    <td><%= transport_guide.customer_id %></td>
    <td><%= transport_guide.foreign_company_id %></td>
    <td><%= transport_guide.employee_id %></td>
    <td><%= transport_guide.service_type_id %></td>
    <td><%= transport_guide.date %></td>
    <td><%= transport_guide.state %></td>
    <td><%= transport_guide.payment_method_id %></td>
    <td><%= link_to 'Show', transport_guide %></td>
    <td><%= link_to 'Edit', edit_transport_guide_path(transport_guide) %></td>
    <td><%= link_to 'Destroy', transport_guide, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

