<h1>Listing foreign_companies</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Company name</th>
    <th>Mobile number</th>
    <th>Email</th>
    <th>Ruc</th>
    <th>Address</th>
    <th>Phone number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @foreign_companies.each do |foreign_company| %>
  <tr>
    <td><%= foreign_company.id %></td>
    <td><%= foreign_company.company_name %></td>
    <td><%= foreign_company.mobile_number %></td>
    <td><%= foreign_company.email %></td>
    <td><%= foreign_company.ruc %></td>
    <td><%= foreign_company.address %></td>
    <td><%= foreign_company.phone_number %></td>
    <td><%= link_to 'Show', foreign_company %></td>
    <td><%= link_to 'Edit', edit_foreign_company_path(foreign_company) %></td>
    <td><%= link_to 'Destroy', foreign_company, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

